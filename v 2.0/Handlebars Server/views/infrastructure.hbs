    <body>
        <h1>Welcome to COS301</h1>

        <div>
            <h2>Here is a list of all the modules</h2>
            {{#each modules}}
                <li><a href="/notify2">{{this}}</a></li>
            {{/each}}
        </div>

        <div>
            <form id = "addForm" action="/submitCS" method="post">
                <ul style="list-style-type: none;">
                    <li>
                        <td><strong>Create Buzz Space</strong></td>
                    </li>
                    <br/>

                    <li>
                        <td>Academic Year</td>
                        <td>
                            <input type="textbox" id="CSyear" name="CSyear">
                        </td>
                    </li>

                    <li>
                        <td>Module ID</td>
                        <td>
                            <input type="textbox" id="CSmodule" name="CSmodule">
                        </td>
                    </li>
                    <li>
                        <td>Name</td>
                        <td>
                            <input type="textbox" id="CSname" name="CSname">
                        </td>
                    </li>

                    </li>

                    <li>
                        <td>
                            <input type="Submit" value="Create">
                        </td>
                        <td>{{message}}</td>
                    </li>

                </ul>
            </form>

            <form id = "addForm" action="/submitRS" method="post">
                <ul style="list-style-type: none;">
                    <li>
                        <td><strong>Close Buzz Space</strong></td>
                    </li>
                    <br/>
                    <li>
                        <td>Module ID</td>
                        <td>
                            <input type="textbox" id="RSmodule" name="RSmodule">
                        </td>
                    </li>
                    <li>
                        <td>User ID</td>
                        <td>
                            <input type="textbox" id="RSid" name="RSid">
                        </td>
                    </li>
                    <li>
                        <td>
                            <input type="Submit" value="Close Space">
                        </td>
                        <td>{{message}}</td>
                    </li>


                </ul>
            </form>


        </div>

    </body>

